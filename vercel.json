{
  "buildCommand": "remix build",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "build/server/index.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ]
}
